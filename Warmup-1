/////// sleepIn //////////////////////////////////////////////////////////////////
public boolean sleepIn(boolean weekday, boolean vacation) {
  if(weekday == false || vacation == true){
   return true;
   }
   return false;
}

/////// monkeyTrouble ////////////////////////////////////////////////////////////
public boolean monkeyTrouble(boolean aSmile, boolean bSmile) {
  if(aSmile == true && bSmile == true || aSmile == false && bSmile == false){
   return true;
  }
  return false;
}
